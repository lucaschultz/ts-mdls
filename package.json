    "author": "Luca Schultz",
    "repository": "github:lucaschultz/ts-mdls",
    "license": "ISC",
    "devDependencies": {
        "@types/benchmark": "^2.1.2",
        "@types/jest": "^29.2.3",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "benchmarkjs-pretty": "^2.0.1",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^29.3.1",
        "ts-jest": "^29.0.3",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.3"
    }
